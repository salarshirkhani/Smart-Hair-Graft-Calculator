{
    "name": "tec/estimator",
    "autoload": {
        "psr-4": {
        "App\\": "app/",
        "Core\\": "app/Core/"
        },
        "exclude-from-classmap": [
        "vendor/symfony/polyfill-php80/Resources/stubs"
        ]
    },

    "authors": [
        {
            "name": "salarshirkhani",
            "email": "salarshirkhany16@gmail.com"
        }
    ],
    "require": {
        "openai-php/client": "^0.14.0",
        "symfony/http-client": "^7.3",
        "nyholm/psr7": "^1.8",
        "vlucas/phpdotenv": "^5.6",
        "symfony/polyfill-php80": "*"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
